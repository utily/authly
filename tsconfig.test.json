{
	"compilerOptions": {
		"skipLibCheck": true,
		"target": "es2020",
		"module": "es2020",
		"strict": true,
		"alwaysStrict": true,
		"noImplicitAny": true,
		"removeComments": false,
		"preserveConstEnums": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitThis": true,
		"strictNullChecks": true,
		"sourceMap": true,
		"outDir": "dist/mjs",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"allowJs": true
	},
	"typeAcquisition": {
		"enable": true
	}
}
